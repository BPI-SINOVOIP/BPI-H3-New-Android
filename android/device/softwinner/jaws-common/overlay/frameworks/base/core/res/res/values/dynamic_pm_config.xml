<!-- See storage config details at http://source.android.com/tech/storage/ -->

<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- removable is not set in nosdcard product -->
    <string-array translatable="false" name="performance_mode">
        <item>/sys/devices/platform/sunxi-budget-cooling/roomage \"1200000 1 1608000 4 1200000 4 1800000 4 1\" </item>
    </string-array>

    <string-array translatable="false" name="normal_mode">
        <item>/sys/devices/platform/sunxi-budget-cooling/roomage \"0 0 0 0 1200000 4 1800000 4 0\" </item>
    </string-array>


    <string-array translatable="false" name="boot_finish">
        <item>/sys/devices/system/cpu/cpu0/cpufreq/scaling_governor \"interactive\" </item>
        <item>/sys/devices/system/cpu/cpu4/cpufreq/scaling_governor \"interactive\" </item>
        <item>/sys/kernel/autohotplug/enable \"1\" </item>
    </string-array>
</resources>
